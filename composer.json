{
	"name": "crizzo/aboutus",
	"type": "phpbb-extension",
	"description": "About us is an extension which adds to phpBB 3.2 an about us or legal notice-page, where you can add your contact details and more.",
	"homepage": "https://github.com/Crizz0/phpbb3-about-us",
	"version": "2.1.0",
	"time": "2017-01-09",
	"license": "GPL-2.0",
	"authors": [{
			"name": "Christian Schnegelberger",
			"username": "Crizzo",
			"email": "kontakt@crizzo.de",
			"homepage": "http://www.crizzo.de",
			"role": "Developer/Founder"
		}],
	"require": {
		"php": ">=5.4,<=7.1"
	},
	"extra": {
		"display-name": "About us",
		"soft-require": {
			"phpbb/phpbb": ">=3.2.0"
		},
		"version-check": {
			"host": "www.crizzo.de",
			"directory": "/phpbbextversion",
			"filename": "phpbb32-ext-aboutus.json"
		}
	}
}
